<?xml version="1.0" encoding="utf-8"?>
<MUSIC>
	<connection>
		<type>DBUS</type>
		<bus_name>org.bansheeproject.Banshee</bus_name>
	    <object_path>/org/mpris/MediaPlayer2</object_path>
	</connection>
	<command>
	        <name>NEXT</name>
	        <type>DBUS</type>
	        <dbus_command>org.mpris.MediaPlayer2.Player.Next</dbus_command>
	</command>
	<command>
	        <name>BACK</name>
	        <type>DBUS</type>
	        <dbus_command>org.mpris.MediaPlayer2.Player.Previous</dbus_command>
	</command>
	<command>
	        <name>PLAY</name>
	        <type>DBUS</type>
	        <dbus_command>org.mpris.MediaPlayer2.Player.PlayPause</dbus_command>
	</command>
	<command>
	        <name>STOP</name>
	        <type>DBUS</type>
	        <dbus_command>org.mpris.MediaPlayer2.Player.Stop</dbus_command>
	</command>
</MUSIC>

